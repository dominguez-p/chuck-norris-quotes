{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/home-page.html",
    "src/register-login.html",
    "src/secret-quotes.html",
    "src/not-found.html",
    "src/all-campings.html",
    "src/show-camping.html",
    "src/new-camping.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
